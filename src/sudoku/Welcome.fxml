<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="630.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sudoku.WelcomeController">
    <stylesheets>
        <URL value="@GamePlayStyle.css" />
    </stylesheets>
   <children>
      <ImageView fitHeight="347.0" fitWidth="353.0" layoutX="197.0" layoutY="14.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../Pics/Logo.png" />
         </image>
      </ImageView>
      <Button fx:id="puzzles" layoutX="316.0" layoutY="373.0" mnemonicParsing="false" onAction="#clickpuzzles" prefHeight="50.0" text="Puzzles" />
      <Button fx:id="exit" layoutX="335.0" layoutY="566.0" mnemonicParsing="false" onAction="#clickexit" prefHeight="50.0" text="Exit" />
      <Button layoutX="288.0" layoutY="502.0" mnemonicParsing="false" onAction="#clickHowToPlay" prefHeight="50.0" text="How To Play" />
      <Button fx:id="growthStatsBtn" layoutX="287.0" layoutY="437.0" mnemonicParsing="false" onAction="#clickGraph" text="Growth Stats" />
   </children>
</AnchorPane>
