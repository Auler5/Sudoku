<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="630.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sudoku.WelcomeController">
    <stylesheets>
        <URL value="@GamePlayStyle.css" />
    </stylesheets>
   <children>
      <ImageView fitHeight="450.0" fitWidth="450.0" layoutX="125.0" layoutY="14.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../Pics/Logo.png" />
         </image>
      </ImageView>
      <Button fx:id="puzzles" layoutX="315.0" layoutY="509.0" mnemonicParsing="false" onAction="#clickpuzzles" text="Puzzles" />
      <Button fx:id="exit" layoutX="335.0" layoutY="571.0" mnemonicParsing="false" onAction="#clickexit" text="Exit" />
   </children>
</AnchorPane>
